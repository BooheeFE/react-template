{
  "name": "react-template",
  "version": "0.1.0",
  "description": "A starter template for React",
  "main": "main.js",
  "scripts": {
    "lint": "yarn lint:es && yarn lint:style",
    "lint:es": "eslint --fix src components config tools --ext '.js,.jsx'",
    "lint:style": "stylelint --fix \"{src,components}/**/*.scss\"",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.config.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack/webpack.config.js",
    "analyzer": "yarn build --plugin webpack-bundle-analyzer/lib/BundleAnalyzerPlugin"
  },
  "author": "simbawu <connect@simbawu.com>",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "autodll-webpack-plugin": "^0.4.2",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.1.5",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-booheefe": "^1.2.7",
    "eslint-plugin-react": "^7.11.1",
    "happypack": "^5.0.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-px-to-viewport": "^0.0.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-logger": "^3.0.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.8.0",
    "stylelint-config-booheefe": "^1.0.4",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BooheeFE/react-template.git"
  },
  "keywords": [
    "react"
  ],
  "bugs": {
    "url": "https://github.com/BooheeFE/react-template/issues"
  }
}
